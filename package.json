{
  "name": "etf.js",
  "version": "1.0.1",
  "description": "An External Term Format Encoder/Decoder, written in pure JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build:browser": "webpack",
    "lint": "eslint --ext ts src --fix",
    "test:lint": "eslint --ext ts src",
    "test": "yarn build && node dist/test/suite.js",
    "test:azure": "yarn build && node dist/test/suite.js",
    "build": "wasm-pack build --target nodejs --release --out-dir wasm/ && tsc -p .",
    "watch": "tsc -p . -w"
  },
  "keywords": [
    "etf",
    "erlpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladfrangu/etf.js.git"
  },
  "author": "Vlad Frangu <kingdgrizzle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladfrangu/etf.js/issues"
  },
  "homepage": "https://github.com/vladfrangu/etf.js#readme",
  "contributors": [
    "Aura Rom√°n <kyradiscord@gmail.com> (https://kyra.dev)"
  ],
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "eslint-config-bamboo": "^1.3.0",
    "typescript": "^3.5.3"
  }
}
